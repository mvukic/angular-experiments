{
  "name": "angular-experiments",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "format": "prettier --write src/app",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run angular-experiments:serve-ssr",
    "serve:ssr": "node dist/angular-experiments/server/main.js",
    "build:ssr": "ng build && ng run angular-experiments:server",
    "prerender": "ng run angular-experiments:prerender",
    "gh-pages": "ng deploy --base-href=/angular-experiments/"
  },
  "private": true,
  "engines": {
    "node": "^18.18.0"
  },
  "dependencies": {
    "@angular/animations": "17.0.0-next.5",
    "@angular/cdk": "17.0.0-next.4",
    "@angular/common": "17.0.0-next.5",
    "@angular/compiler": "17.0.0-next.5",
    "@angular/core": "17.0.0-next.5",
    "@angular/forms": "17.0.0-next.5",
    "@angular/platform-browser": "17.0.0-next.5",
    "@angular/platform-browser-dynamic": "17.0.0-next.5",
    "@angular/platform-server": "17.0.0-next.5",
    "@angular/router": "17.0.0-next.5",
    "@angular/ssr": "17.0.0-next.1",
    "@nguniversal/express-engine": "16.2.0",
    "express": "4.18.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.0-next.5",
    "@angular/cli": "17.0.0-next.5",
    "@angular/compiler-cli": "17.0.0-next.5",
    "@ngneat/spectator": "15.0.1",
    "@nguniversal/builders": "16.2.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.5",
    "@types/node": "20.6.3",
    "angular-cli-ghpages": "1.0.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.6.3"
